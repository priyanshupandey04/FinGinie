datasource db {
  provider = "postgresql" // or "sqlite"
}

generator client {
  provider = "prisma-client"
  output   = "../src/generated"
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
  createdAt DateTime @default(now())
  password String
  image String?
  phone String?
}
